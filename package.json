{
  "name": "tailwind-css-nested",
  "version": "0.0.0",
  "license": "GNU General Public License v3.0",
  "packageManager": "yarn@3.5.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint 'src/**/{*.ts,*.tsx}'",
    "build:esbuild": "node scripts/build.mjs",
    "typegen": "tsc --excludeFiles src/index.test.ts",
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production npm-run-all clean typegen build:esbuild",
    "build:dev": "cross-env NODE_ENV=development npm-run-all clean typegen build:esbuild"
  },
  "dependencies": {
    "@types/node": "^18.15.11",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.21.3",
    "@types/jest": "^29.5.0",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.14",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  }
}
